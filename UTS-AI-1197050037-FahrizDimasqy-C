i persediaan terbanyak dengan rumus:\n","# μpmtTURUN[x] = y - persediaan barang tersedikit(pb_min) / 500\n","# psd_banyak adalah persediaan barang terbanyak\n","def psd_banyak(y):\n","  nilai = (y - pb_min)/500\n","  return nilai\n","print(\"Permintaan Turun = \",psd_banyak(y))\n","\n","#Aturan proses produksi\n","print(\"\")\n","print(\"Aturan proses produksi perusahaan tersebut menggunakan 4 aturan sebagai berikut:\")\n","print(\"[R1] IF permintaan TURUN AND Persediaan BANYAK, THEN Produksi Barang dr\");\n","print(\"[R2] IF permintaan TURUN AND Persediaan SEDIKIT, THEN Produksi Barang BERKURANG\");\n","print(\"[R3] IF permintaan NAIK AND Persediaan BANYAK, THEN Produksi Barang BERTAMBAH\");\n","print(\"[R4] IF permintaan NAIK AND Persediaan SEDIKIT, THEN Produksi Barang BERTAMBAH\\n\");\n","\n","#catatan a = alpha\n","#R1\n","print(\"Rule 1 [R1]\")\n","a_predikat1 = min(pmt_turun(x), psd_banyak(y))\n","print(\"Aplha Predikat 1 = \", a_predikat1)\n","z1 = pd_max -  a_predikat1 * (pd_max - pd_min)\n","print(\"z1 = \", int(z1))\n","\n","# R2\n","print(\"Rule 2 [R2]\")\n","a_predikat2 = min(pmt_turun(x), psd_sedikit(y))\n","print(\"Aplha Predikat 2 = \", a_predikat2)\n","z2 = pd_max -  a_predikat2 * (pd_max - pd_min)\n","print(\"z2 = \", int(z2))\n","\n","# R3\n","print(\"Rule 3 [R3]\")\n","a_predikat3 = min(pmt_naik(x), psd_banyak(y))\n","print(\"Aplha Predikat 3 = \", a_predikat3)\n","z3 = a_predikat3 * (pd_max - pd_min) + pd_min\n","print(\"z3 = \", int(z3))\n","\n","# rule 4\n","print(\"Rule 4 [R4]\")\n","a_predikat4 = min(pmt_naik(x), psd_sedikit(y))\n","print(\"Aplha Predikat 4 = \", a_predikat4)\n","z4 = a_predikat4 * (pd_max - pd_min) + pd_min\n","print(\"z4 = \", int(z4))\n","print(\"\")\n","\n","# Mencari nilai z akhir:\n","#caranya dengan merata-rata semua z berbobot:\n","print(\"Menghitung z akhir\")\n","print(\"caranya dengan merata-rata semua z berbobot:\")\n","print(\"sehingga menjadi seperti dibawah:\")\n","print(\"z = ((a_predikat1 * z1) + (a_predikat2 * z2) + (a_predikat3 * z3) + (a_predikat4 * z4)) / ((a_predikat1 + a_predikat2 + a_predikat3 + a_predikat4))\")\n","z = ((a_predikat1 * z1) + (a_predikat2 * z2) + (a_predikat3 * z3) + (a_predikat4 * z4)) / ((a_predikat1 + a_predikat2 + a_predikat3 + a_predikat4))\n","print(\"Jadi, jumlah makanan jenis ABC yang harus diproduksi sebanyak\", int(z), \"Kemasan\")\n","\n","\n"],"execution_count":5,"outputs":[{"output_type":"stream","name":"stdout","text":["Permintaan Turun =  0.25\n","Permintaan Turun =  0.75\n","Permintaan Turun =  0.6\n","Permintaan Turun =  0.4\n","\n","Aturan proses produksi perusahaan tersebut menggunakan 4 aturan sebagai berikut:\n","[R1] IF permintaan TURUN AND Persediaan BANYAK, THEN Produksi Barang dr\n","[R2] IF permintaan TURUN AND Persediaan SEDIKIT, THEN Produksi Barang BERKURANG\n","[R3] IF permintaan NAIK AND Persediaan BANYAK, THEN Produksi Barang BERTAMBAH\n","[R4] IF permintaan NAIK AND Persediaan SEDIKIT, THEN Produksi Barang BERTAMBAH\n","\n","Rule 1 [R1]\n","Aplha Predikat 1 =  0.25\n","z1 =  5750\n","Rule 2 [R2]\n","Aplha Predikat 2 =  0.25\n","z2 =  5750\n","Rule 3 [R3]\n","Aplha Predikat 3 =  0.4\n","z3 =  4000\n","Rule 4 [R4]\n","Aplha Predikat 4 =  0.6\n","z4 =  5000\n","\n","Menghitung z akhir\n","caranya dengan merata-rata semua z berbobot:\n","sehingga menjadi seperti dibawah:\n","z = ((a_predikat1 * z1) + (a_predikat2 * z2) + (a_predikat3 * z3) + (a_predikat4 * z4)) / ((a_predikat1 + a_predikat2 + a_predikat3 + a_predikat4))\n","Jadi, jumlah makanan jenis ABC yang harus diproduksi sebanyak 4983 Kemasan\n"]}]}]}